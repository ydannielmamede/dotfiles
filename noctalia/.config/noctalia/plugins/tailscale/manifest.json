{
  "id": "tailscale",
  "name": "Tailscale",
  "version": "1.1.2",
  "minNoctaliaVersion": "4.0.0",
  "author": "nineluj",
  "license": "MIT",
  "repository": "https://github.com/noctalia-dev/noctalia-plugins",
  "description": "Show Tailscale status in the menu bar.",
  "tags": [
    "Bar",
    "Network",
    "Panel"
  ],
  "entryPoints": {
    "main": "Main.qml",
    "barWidget": "BarWidget.qml",
    "panel": "Panel.qml",
    "settings": "Settings.qml"
  },
  "dependencies": {
    "plugins": []
  },
  "metadata": {
    "defaultSettings": {
      "refreshInterval": 5000,
      "compactMode": false,
      "showIpAddress": true,
      "showPeerCount": true,
      "hideDisconnected": false,
      "terminalCommand": "",
      "pingCount": 5,
      "defaultPeerAction": "copy-ip"
    }
  }
}
